{
  "name": "tree-sitter-ssh-config",
  "version": "0.1.1",
  "license": "MIT",
  "description": "OpenSSH config grammar for tree-sitter",
  "repository": "ObserverOfTime/tree-sitter-ssh-config",
  "author": {
    "name": "ObserverOfTime",
    "email": "chronobserver@disroot.org",
    "url": "https://observeroftime.github.io/"
  },
  "keywords": [
    "tree-sitter",
    "parser",
    "lexer"
  ],
  "scripts": {
    "generate": "tree-sitter generate",
    "highlight": "tree-sitter highlight",
    "parse": "tree-sitter parse",
    "test": "tree-sitter test"
  },
  "main": "bindings/node",
  "dependencies": {
    "nan": "^2.17.0"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.20.8"
  },
  "tree-sitter": [
    {
      "scope": "text.ssh_config",
      "content-regex": "^\\s*IdentityFile"
    }
  ]
}
